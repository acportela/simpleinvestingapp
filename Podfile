# Uncomment the next line to define a global platform for your project
platform :ios, '11.4'

use_frameworks!

def all_pods
    pod 'SwiftLint'
    pod 'Alamofire', '~> 4.8'
    pod 'SnapKit'
end

target 'SimpleInvesting' do
    all_pods
end

target 'SimpleInvestingTests' do
    all_pods
    pod 'Nimble-Snapshots'
    pod 'Quick', '~>2.1.0'
    pod 'Nimble', '~>8.0.1'
end
